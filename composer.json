{
    "name": "manomano-tech/correlation-ids-httplug",
    "description": "HTTPlug plugin for manomano-tech/correlation-ids package",
    "keywords": [
        "correlation",
        "correlation-id",
        "tracing",
        "HTTPlug",
        "plugin",
        "middleware"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "manomano-tech/correlation-ids": "^1.0",
        "php-http/client-common": "^1.0",
        "php-http/client-implementation": "^1.0"
    },
    "require-dev": {
        "guzzlehttp/psr7": "^1.4.2",
        "php-http/mock-client": "^1.1.0",
        "phpunit/phpunit": "^7.3",
        "symfony/debug": "^4.1",
        "symfony/phpunit-bridge": "^4.1"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "ManoManoTech\\CorrelationIdHTTPlug\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ManoManoTech\\CorrelationIdHTTPlug\\Tests\\": "tests/"
        }
    }
}

